<%= form_with model: @user do |form|%>
<%= form.label:name %>
<%= form.text_field :name %>
<br/>
<%= form.label:email %>
<%=form.text_field :email %>
<br/>
<%= form.label:Phone %>
<%=form.text_field :mobile %>
<br/>
<%= form.label:Branch %>
<%=form.text_field :branch %>
<br/>
<%= form.label:CGPA%>
<%=form.text_field :cgpa %>
<br/>
<%= form.file_field :pictures, multiple: true %>
<br/>
<%= form.submit "submit" %>
<% end %>